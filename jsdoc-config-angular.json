{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": [
			"src"
		]
	},
	"plugins": [
		"node_modules/jsdoc/plugins/markdown",
		"node_modules/angular-jsdoc/common/plugins/ngdoc"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	},
	"opts": {
		"encoding": "utf8",
		"destination": "output",
		"recurse": true
	},
	"markdown.hardwrap": true,
	"markdown": {
		"hardwrap": true
	}
}
